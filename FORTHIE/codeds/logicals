STRING "Logical ∧(and) and ∨(or) and ~(not)." PRINTNL

STRING "1 1 = 4 5 < ∧" PRINTNL
INT 1 1 = 4 5 < ∧ IF
    STRING "AND!" PRINTNL
END
STRING "1 1 = 4 5 = ∧" PRINTNL
INT 1 1 = 4 5 = ∧ IF
    STRING "AND!" PRINTNL
ELSE
    STRING "No and.." PRINTNL
END

STRING "1 1 ≠ 5 4 > ∨" PRINTNL
INT 1 1 ≠ 5 4 > ∨ IF
    STRING "OR!" PRINTNL
END
STRING "1 2 = 4 2 + ∨" PRINTNL
INT 1 2 = 4 2 + ∨ IF
    STRING "OR!" PRINTNL
ELSE
    STRING "No or.." PRINTNL
END

STRING "1 NOT" PRINTNL
INT 1 NOT IF
ELSE
    STRING "Not not!" PRINTNL
END
STRING "0 NOT" PRINTNL
INT 0 NOT IF
    STRING "Yes not!" PRINTNL
END
STRING "5 NOT is an error." PRINTNL
INT 5 NOT PRINTNL

