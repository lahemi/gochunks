STRING "Some stack operations" PRINTNL

INT 1 2 3  .S STRING " start" PRINTNL
INT DROP   .S STRING " DROP!" PRINTNL
INT SWAP   .S STRING " swap"  PRINTNL
INT DUP    .S STRING " dup"   PRINTNL
INT ROT    .S STRING " rot"   PRINTNL

INT 3 4 5  .S STRING " add some stuff for..." PRINTNL
INT 3 INT PICK .S STRING " pick!"  PRINTNL

